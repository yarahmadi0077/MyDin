<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.controllers.FollowingsListController">
    <Label style="-fx-font-size: 16px; -fx-font-weight: bold;" text="Followings List" />
    <TableView fx:id="followingsTable">
        <columns>
            <TableColumn fx:id="colFullName" text="Full Name" />
            <TableColumn fx:id="colEmail" text="Email" />
        </columns>
    </TableView>
    <Button onAction="#closeWindow" text="Close">
      <font>
         <Font name="System Bold Italic" size="12.0" />
      </font></Button>
</VBox>
